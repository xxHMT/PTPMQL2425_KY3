@model DemoMVC.Models.HeThongPhanPhoi

@{
    ViewData["Title"] = "Create";
}

<h1>Create New HTPP</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MaHTPP" class="control-label"></label>
                <input type="text" name="MaHTPP" value="@ViewBag.NewMaHTPP" readonly="readonly" class="form-control" />
                <span asp-validation-for="MaHTPP" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TenHTPP" class="control-label"></label>
                <input asp-for="TenHTPP" class="form-control">
                <span asp-validation-for="TenHTPP" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back To List</a>
</div>

@section Scripts{
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}